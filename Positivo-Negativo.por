programa {
  funcao inicio() {
    inteiro numero
    escreva("Digite o número: ")
    leia(numero)
    se(numero>0){
      escreva("Seu valor é positivo!")
    }senao se (numero<0){
      escreva("Seu valor é negativo!")
      }senao{
        escreva("Seu valor é zero!")
      }
    }
    }
    
  }
}
